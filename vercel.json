{
  "rewrites": [
    {
      "source": "/index.html",
      "destination": "/src/index.html"
    },
    {
      "source": "/main.js",
      "destination": "/src/main.js"
    },
    {
      "source": "/style.css",
      "destination": "/src/style.css"
    },
    {
      "source": "/create",
      "destination": "/api/create"
    },
    {
      "source": "/.*",
      "destination": "/api/url"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/index.html",
      "permanent": true
    }
  ]
}